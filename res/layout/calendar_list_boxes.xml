<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp"
    android:orientation="vertical" >
	
    <View
        android:layout_width="match_parent"
        android:layout_height="6dp" />
	
    <TableLayout android:id="@+id/CalendarListClick"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:stretchColumns="1,3"
        android:shrinkColumns="1,3"
        style="?android:buttonStyle" >
        
        <TableRow
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content" >
	        
            <TextView
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:layout_gravity="left|center_vertical"
	            android:textSize="12sp"
	            android:text="@string/calendarStart"
	            android:textColor="?pageText" />
            
            <TextView android:id="@+id/CalendarListStart"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="5"
		        android:layout_gravity="left|center_vertical"
	            android:text="@string/dummy"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="?pageText" />
            
            <TextView
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:layout_gravity="left|center_vertical"
	            android:textSize="12sp"
	            android:text="@string/calendarDate"
	            android:textColor="?pageText" />
            
            <TextView android:id="@+id/CalendarListDate"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="5"
		        android:layout_gravity="left|center_vertical"
	            android:text="@string/dummy"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="?pageText" />
            
        </TableRow>
        
        <TableRow
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content" >
            
            <TextView
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:layout_gravity="left|center_vertical"
	            android:textSize="12sp"
	            android:text="@string/calendarEnd"
	            android:textColor="?pageText" />
            
            <TextView android:id="@+id/CalendarListEnd"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="5"
		        android:layout_gravity="left|center_vertical"
	            android:text="@string/dummy"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="?pageText" />
            
            <TextView
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:layout_gravity="left|center_vertical"
	            android:textSize="12sp"
	            android:text="@string/calendarRoom"
	            android:textColor="?pageText" />
            
            <TextView android:id="@+id/CalendarListRoom"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="5"
		        android:layout_gravity="left|center_vertical"
	            android:text="@string/dummy"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="?pageText" />
            
        </TableRow>
        
        <TableRow
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content" >
            
            <TextView
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:layout_gravity="left|center_vertical"
	            android:textSize="12sp"
	            android:text="@string/calendarType"
	            android:textColor="?pageText" />
            
            <TextView android:id="@+id/CalendarListType"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="5"
		        android:layout_gravity="left|center_vertical"
	            android:text="@string/dummy"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="?pageText" />
            
            <TextView
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:layout_gravity="left|center_vertical"
	            android:textSize="12sp"
	            android:text="@string/calendarSubject"
	            android:textColor="?pageText" />
            
            <TextView android:id="@+id/CalendarListSubject"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="5"
		        android:layout_gravity="left|center_vertical"
	            android:text="@string/dummy"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="?pageText" />
            
        </TableRow>
        
        <TableRow
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content" >
            
            <TextView
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:layout_gravity="left|center_vertical"
	            android:textSize="12sp"
	            android:text="@string/calendarPercentShort"
	            android:textColor="?pageText" />
            
            <TextView android:id="@+id/CalendarListPercent"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="5"
		        android:layout_gravity="left|center_vertical"
	            android:text="@string/dummy"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="?pageText" />
            
            <TextView
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:layout_gravity="left|center_vertical"
	            android:textSize="12sp"
	            android:text="@string/calendarGrade"
	            android:textColor="?pageText" />
            
            <TextView android:id="@+id/CalendarListGrade"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:layout_weight="5"
		        android:layout_gravity="left|center_vertical"
	            android:text="@string/dummy"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="?pageText" />
            
        </TableRow>
        
    </TableLayout>
	
</LinearLayout>